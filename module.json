{
  "id": "bx-factions",
  "title": "BX-Friendly Journal Templates",
  "description": "A lightly-opinionated set of journal page templates, for use with games of the Basic-Expert lineage",
  "authors": [
    {
      "name": "wyrmisis",
      "discord": "wyrmisis"
    }
  ],
  "esmodules": [
    "dist/js/bundle.min.js"
  ],
  "styles": [
    "dist/css/bundle.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "compatibility": {
    "minimum": 11,
    "verified": 12,
    "maximum": 12
  },
  "license": "LICENSE",
  "version": "auto-replaced",
  "url": "auto-replaced",
  "manifest": "auto-replaced",
  "download": "auto-replaced",
  "documentTypes": {
    "JournalEntryPage": {
      "faction": {
        "htmlFields": ["description", "notes", "renforcements", "territory", "relationships", "goals"],
        "filePathFields": {
          "img": ["IMAGE"]
        }
      },
      "roomKey": {
        "htmlFields": ["description", "inhabitantNotes", "treasureNotes", "traps", "secrets" ],
        "filePathFields": {
          "img": ["IMAGE"]
        }
      },
      "characterClass": {
        "htmlFields": ["description"],
        "filePathFields": {
          "img": ["IMAGE"]
        }
      }
    }
  }
}