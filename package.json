{
  "name": "bx-factions",
  "version": "0.0.1",
  "description": "UI build tooling and dependencies for bx-factions -- see module.json",
  "scripts": {
    "build": "npm-run-all -p  build:js build:css",
    "build:js": "rollup --config ./rollup.config.js",
    "build:css": "postcss src/style/index.css -o dist/css/bundle.css",
    "start": "npm-run-all -p watch:js watch:css",
    "watch:js": "rollup --config ./dev.rollup.config.js --watch",
    "watch:css": "postcss src/style/index.css -o dist/css/bundle.css --watch"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "author": "wyrmisis",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
    "autoprefixer": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "cssnano": "^5.1.7",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "postcss-inherit": "^4.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-reporter": "^7.0.5",
    "rollup": "^2.72.1",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^14.8.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0"
  }
}
